{
  "name": "@zeainc/zea-tree-view",
  "version": "0.0.5",
  "license": "MIT",
  "module": "dist/zeatreeview.esm.js",
  "browser": "dist/zeatreeview.umd.production.min.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch --format esm,umd --name zeaTreeView",
    "build": "tsdx build --format esm,umd --name zeaTreeView",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "yarn run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@zeainc/zea-cad": "^4",
    "@zeainc/zea-engine": "^4"
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/tree-view.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tree-view.esm.js",
      "limit": "10 KB"
    }
  ]
}
